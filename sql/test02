SELECT ATTRIBUTE(S)
cust, prod, quant
NUMBER OF GROUPING VARIABLES(n):
3
GROUPING ATTRIBUTES(V):
cust, prod
F-VECT([F]):
1_avg_prod, 2_avg_prod, 3_avg_prod
SELECT CONDITION-VECT([sigma]):
1_cust=cust and 1_state='NY'
2_cust=cust and 2_state='CT'
3_cust=cust and 3_state='NJ'
HAVING CONDITION(G):
1_avg_prod > 2_avg_prod and 1_avg_prod > 3_avg_prod 
WHERE CONDITIONS:
year = 1997